    Description: Shape Property Set for ObjModelLoaderWrapper
-->

<Set>
    <Property
        Name="FilePath"
        Type="java.lang.String"
        Getter="getFilePath"
        Setter="setFilePath"
        DefaultValue="null"
        DisplayName="File Path"
        Expert="false"
        Preferred="true"
        ShortDescription="Model file path"
        PropertyEditorClass="org.netbeans.modules.plethora.properties.ModelFilePropertyEditor"
    />
    <Property
        Name="LoaderClass"
        Type="java.lang.Class"
        Getter="getLoaderClass"
        Setter="null"
        DefaultValue="null"
        DisplayName="Loader Class"
        Expert="true"
        Preferred="false"
        ShortDescription="Model loader class"
        PropertyEditorClass=""
    />
</Set>