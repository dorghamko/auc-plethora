                    <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/plethora/resources/lg3d-icon.png"/>
                    <attr name="SystemFileSystem.icon32" urlvalue="nbresloc:/org/netbeans/modules/plethora/resources/lg3d-icon32.png"/>
                    <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.plethora.resources.Bundle"/>
                    <attr name="instantiatingIterator" methodvalue="org.netbeans.modules.plethora.project.Lg3dApplicationWizardIterator.createIterator"/>
                    <attr name="instantiatingWizardURL" urlvalue="nbresloc:/org/netbeans/modules/plethora/resources/Lg3dApplication.html"/>
                    <attr name="template" boolvalue="true"/>
                </file>
            </folder>
        </folder>
    </folder>
    <folder name="Loaders">
        <folder name="text">
            <folder name="x-lgcfg">
