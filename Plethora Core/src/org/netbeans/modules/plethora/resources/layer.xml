                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.plethora.resources.Bundle"/>
                <attr name="SystemFileSystem.icon" urlvalue="nbresloc:/org/netbeans/modules/plethora/resources/lg3d-icon.png"/>
