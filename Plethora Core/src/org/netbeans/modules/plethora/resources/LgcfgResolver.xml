<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MIME-resolver PUBLIC "-//NetBeans//DTD MIME Resolver 1.0//EN" "http://www.netbeans.org/dtds/mime-resolver-1_0.dtd">

<!--
    Document   : LgcfgResolver.xml
    Created on : September 29, 2006
    Author     : Mohamed El-Geish
    Description: Resolver to match the lgcfg extension with the text/x-lgcfg MIME
-->

<MIME-resolver>
    <file>
        <ext name="lgcfg"/>
        <resolver mime="text/x-lgcfg"/>
    </file>
</MIME-resolver>
